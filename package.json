{
  "name": "exchange",
  "version": "1.0.2",
  "description": "Exchange rates calculator",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "prebuild": "npm run lint && npm run test",
    "build": "webpack",
    "test": "jest",
    "lint": "eslint src",
    "prettier": "prettier --write",
    "publish": "gh-pages -d dist",
    "release": "npm run build && npm version minor && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gleb-lobastov/exchange.git"
  },
  "keywords": [
    "currency",
    "exchange",
    "rates",
    "calculator"
  ],
  "author": "Gleb Lobastov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gleb-lobastov/exchange/issues"
  },
  "homepage": "https://github.com/gleb-lobastov/exchange#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.5.1",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "commitlint": "^7.5.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.6.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "npm run prettier",
      "git add"
    ]
  }
}
